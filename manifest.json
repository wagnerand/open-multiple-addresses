{
  "manifest_version": 2,
  "name": "Open Multiple Addresses",
  "version": "1.2.0",
  "description": "Open multiple addresses at once",
  "icons": {
    "48": "img/icon48.png",
    "96": "img/icon96.png"
  },
  "applications": {
    "gecko": {
      "id": "webext-open-multiple@andreaswagner.org",
      "strict_min_version": "45.0"
    }
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Open multiple addresses",
    "default_icon": {
      "16": "img/icon16.png",
      "19": "img/icon19.png",
      "32": "img/icon32.png",
      "38": "img/icon38.png",
      "64": "img/icon64.png"
    }
  },
  "permissions": [
    "tabs"
  ]
}
