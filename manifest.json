{
  "manifest_version": 2,
  "name": "Open Multiple Addresses",
  "version": "1.3.1",
  "description": "Open multiple addresses at once",
  "icons": {
    "48": "img/icon-dark.svg",
    "96": "img/icon-dark.svg"
  },
  "applications": {
    "gecko": {
      "id": "webext-open-multiple@andreaswagner.org",
      "strict_min_version": "45.0"
    }
  },
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup.html",
    "default_title": "Open multiple addresses",
    "theme_icons": [{
        "light": "img/icon-light.svg",
        "dark": "img/icon-dark.svg",
        "size": 16
      },
      {
        "light": "img/icon-light.svg",
        "dark": "img/icon-dark.svg",
        "size": 32
      }]
  },
  "permissions": [
    "clipboardRead",
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+T"
      }
    }
  }
}
