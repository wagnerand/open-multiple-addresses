{
  "manifest_version": 2,
  "name": "Open Multiple Addresses",
  "version": "1.2.0",
  "description": "Open multiple addresses at once",
  "icons": {
    "48": "img/icon48.png",
    "96": "img/icon96.png"
  },
  "applications": {
    "gecko": {
      "id": "webext-open-multiple@andreaswagner.org",
      "strict_min_version": "45.0"
    }
  },
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup.html",
    "default_title": "Open multiple addresses",
    "theme_icons": [{
        "light": "img/icon16-light.png",
        "dark": "img/icon16-dark.png",
        "size": 16
      },
      {
        "light": "img/icon32-light.png",
        "dark": "img/icon32-dark.png",
        "size": 32
      }]
  },
  "permissions": [
    "clipboardRead",
    "tabs"
  ]
}
